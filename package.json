{
  "name": "@soyagaci/models",
  "version": "1.1.1",
  "description": "Soyağacı ile çalışmak için gerekli olan temel modeller",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soyagaci/models.git"
  },
  "keywords": [
    "soyagaci",
    "models",
    "typescript"
  ],
  "author": "Yengas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soyagaci/models/issues"
  },
  "homepage": "https://github.com/soyagaci/models#readme",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.spec.ts"
    ]
  },
  "files": [
    "dist/"
